<Window x:Class="Tienda.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tienda"
        mc:Ignorable="d"
        Title="MainWindow" Height="191.949" Width="800">
    <StackPanel>
        <TextBox Name="nombreTextBox" Text="Inserte nombre"></TextBox>
        <TextBox Name="codigoTextBox" Text="Inserte código producto"></TextBox>
        <TextBox Name="valorTextBox" Text="Inserte valor producto"></TextBox>
        <Button Name="insertButton" Content="Insertar producto" Click="insertButton_Click"></Button>
        <TextBox x:Name="successTextBox" Height="38" TextWrapping="Wrap" Text="" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#FF57A223"/>
    </StackPanel>
</Window>
